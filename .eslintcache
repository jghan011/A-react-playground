 console.log(this.props)\n        return (\n          <span className='Tooltip'>\n            <span\n              className='Tooltip-content'\n              style={{ color: this.props.color }}\n            >\n              {this.props.children}\n            </span>\n            <div className='Tooltip-message'>\n              {this.props.message}\n            </div>\n          </span>\n        )\n      }\n}\n//props accessed by this.props\n//class components use uses this.props and props no longer act as perameter\n\n// export default Tooltip;\n\nexport default TooltipClass;",["59","60"],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/composition/Messages.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state/TheDate.js",["61","62"],"import { render } from '@testing-library/react';\nimport React, { Component } from 'react';\n\nclass TheDate extends React.Component {\n    constructor(props) {\n        super(props)\n        this.state = { datetime: new Date() };\n        console.log('constructor')\n      }\n\n\n      componentDidMount()  {\n          console.log('componentDidMount')\n      this.interval =  setInterval(() => {\n          //basically  everything inside this.interval replaces this.state replace\n          console.log('setInterval')\n        this.setState({\n                datetime: new Date() \n            })\n        }, 1000);\n\n      }\n\n      componentWillUnmount() {\n          clearInterval(this.interval)\n      }\n\n      render() {\n          console.log('render')\n\n        return (\n          <div>{this.state.datetime.toLocaleString()}</div>\n        )\n      }\n\n}\n\n\n\n\n\n\nexport default TheDate;\n\n","/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/Counter/Counter.js",["63"],"import React from 'react'\nimport ReactDOM from 'react-dom';\n\nclass Counter extends React.Component {\n\n    constructor(props){\n         console.log('props in construct