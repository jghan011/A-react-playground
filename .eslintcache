 you click the number increases\n        this.setState({\n            count: newNumber\n        }\n        )\n    }\n\n\n    render() {\n        return (\n            <div>\n                <div>Count: {this.state.count} </div>\n                <button onClick={() => this.handleButtonClick()}>incrememnt</button>\n            </div>\n        )\n    }\n\n\n}\n\n\n//on click is  an event handling prop specifically for when eleements are clicked. There is also: onSubmit,onHover,onBlur\n\nexport default Counter;","/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state-drills/HelloWorld.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state-drills/Bomb.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state-drills/RouletteGun.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state/Tabs/Tabs.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/ShoppingTing/AppOne.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/ShoppingTing/AddItemForm.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/ShoppingTing/ShoppingList.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/ShoppingTing/ShoppingItem.js",[],{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":2,"column":8,"nodeType":"82","messageId":"83","endLine":2,"endColumn":13},{"ruleId":"80","severity":1,"message":"84","line":4,"column":10,"nodeType":"82","messageId":"83","endLine":4,"endColumn":17},{"ruleId":"76","replacedBy":"85"},{"ruleId":"78","replacedBy":"86"},{"ruleId":"80","severity":1,"message":"87","line":1,"column":10,"nodeType":"82","messageId":"83","endLine":1,"endColumn":16},{"ruleId":"80","severity":1,"message":"88","line":2,"column":17,"nodeType":"82","messageId":"83","endLine":2,"endColumn":26}