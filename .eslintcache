,[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state/TheDate.js",["74","75"],"import { render } from '@testing-library/react';\nimport React, { Component } from 'react';\n\nclass TheDate extends React.Component {\n    constructor(props) {\n        super(props)\n        this.state = { datetime: new Date() };\n        console.log('constructor')\n      }\n\n\n      componentDidMount()  {\n          console.log('componentDidMount')\n      this.interval =  setInterval(() => {\n          //basically  everything inside this.interval replaces this.state replace\n          console.log('setInterval')\n        this.setState({\n                datetime: new Date() \n            })\n        }, 1000);\n\n      }\n\n      componentWillUnmount() {\n          clearInterval(this.interval)\n      }\n\n      render() {\n          console.log('render')\n\n        return (\n          <div>{this.state.datetime.toLocaleString()}</div>\n        )\n      }\n\n}\n\n\n\n\n\n\nexport default TheDate;\n\n","/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/Counter/Counter.js",["76"],"import React from 'react'\nimport ReactDOM from 'react-dom';\n\nclass Counter extends React.Component {\n\n    constructor(props){\n         console.log('props in constructor', props)\n\n        super(props)\n        this.state = {\n            count: 0\n        }\n    };\n    \n\n    handleButtonClick = () => {\n        const newNumber = this.state.count + 1;\n        console.log('props in handleButtonClick', this.props)//will refrer to count={123} in App.js \n        console.log('state in handleButtonClick', this.state)//refers to count:{} inside  the return () and every time you click the number increases\n        this.setState({\n            count: newNumber\n        }\n        )\n    }\n\n\n    render() {\n        return (\n            <div>\n                <div>Count: {this.state.count} </div>\n                <button onClick={() => this.handleButtonClick()}>incrememnt</button>\n            </div>\n        )\n    }\n\n\n}\n\n\n//on click is  an event handling prop specifically for when eleements are clicked. There is also: onSubmit,onHover,onBlur\n\nexport default Counter;","/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state-drills/HelloWorld.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state-drills/Bomb.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state-drills/RouletteGun.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/state/Tabs/Tabs.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/ShoppingTing/AppOne.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/ShoppingTing/AddItemForm.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/ShoppingTing/ShoppingList.js",[],"/Users/jasonfrimpong/Documents/Thinkful-Folder/npmTing/react-playground/src/ShoppingTing/ShoppingItem.js",[],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":2,"column":8,"nodeType":"83","messageId":"84","endLine":2,"endColumn":13},{"ruleId":"81","severity":1,"message":"85","line":4,"column":10,"nodeType":"83","messageId":"84","endLine":4,"endColumn":17},{"ruleId":"77","replacedBy":"86"},{"ruleId":"79","replacedBy":"87"},{"ruleId":"81","severity":1,"message":"88","line":1,"column":10,"nodeType":"83","messageId":"84","endLine":1,"endColumn":16},{"ruleId":"81","severity":1,"message":"89","line":2,"column":17,"nodeType":"83","messageId":"84","endLine":2,"endColumn":26},{"ruleId":"81","severity":1,"message":"90","line":2,"column":8,"nodeType":"83","messageId":"84","endLine":2,"endColumn":16},"no-native-reassign",["91"],"no-negated-in-lhs",["92"],"no-unused-vars","'Split' is defined but never used.","Identifier","unusedVar","'Tooltip' is defined but 